<!-- drawable/ic_launcher_new.xml -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="1dp"
        android:left="2dp"
        android:right="1dp"
        android:top="2dp">
        <shape>
            <solid android:color="#334d00" />
            <stroke
                android:width="2dp"
                android:color="#334d00" />
            <corners android:radius="8dp" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="1dp"
        android:right="3dp"
        android:top="1dp">
        <shape>
            <gradient
                android:angle="270"
                android:startColor="#46c38d"
                android:endColor="#296342"
                android:centerColor="#3a8867"/>
            <stroke
                android:width="2.4dp"
                android:color="#B18904" />
            <corners android:radius="8dp" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="2dp">
        <inset
            android:drawable="@drawable/ic_iwillnot" >
        </inset>
    </item>
</layer-list>